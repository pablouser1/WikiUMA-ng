{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "pablouser1/wikiuma-ng",
    "description": "Valoraciones a profesores de la UMA",
    "type": "project",
    "license": "AGPL-3.0-or-later",
    "scripts": {
        "bulma": "misc/bulma.sh"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Pablo Ferreiro",
            "email": "me@pabloferreiro.es"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "ext-mbstring": "*",
        "ext-curl": "*",
        "league/route": "^6.2",
        "laminas/laminas-diactoros": "^3.6",
        "laminas/laminas-httphandlerrunner": "^2.12",
        "league/plates": "^3.6",
        "illuminate/database": "^12.28",
        "altcha-org/altcha": "^1.1",
        "league/climate": "^3.10",
        "league/commonmark": "^2.7",
        "phpmailer/phpmailer": "^6.10",
        "npm-asset/bulma": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24"
    },
    "suggest": {
        "ext-apcu": "APCu cache support",
        "ext-redis": "Redis cache support"
    }
}
